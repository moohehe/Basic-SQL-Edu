<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!-- 게시판 처리 SQL -->
<mapper namespace="com.basicsqledu.www.mapper.QuizMapper">

	<!-- View를 활용하는 데이터로 -->
	<select id="getAnimal2" resultType="animal">
		SELECT *	
		FROM
			animal_view
	</select>
	
	<select id="getAnimal" parameterType="String" resultType="animal">
		SELECT *	
		FROM
			${value}
	</select>
	

	
</mapper>